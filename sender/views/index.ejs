<!DOCTYPE html>
<html>
  <head>
    <title>AYD.IO</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script src="/javascripts/socket.io.js"></script>
    <script src="/javascripts/lodash.js"></script>
    <script src="/javascripts/jquery.1.10.min.js"></script>
    <script src="/javascripts/app.js"></script>
  </head>
  <body>
    <div class="wrapper card-shadow">
      <header class="control-header">
        <div class="player-controls">
          <div class="button_bwd dib"></div>
          <div id="playPause" class="button_playpause dib"></div>
          <div class="button_fwd dib"></div>
          <div class="sound_control">
            <div class="speaker_quiet"></div>
            <div class="speaker_loud"></div>
            <div class="track">
              <div class="active"></div>
            </div>
          </div>
        </div>
        <div class="status-controls">
          <div class="artist-line">
            Foobias Barstrap
          </div>
          <div class="track-line">
            Do my fooba fooba thingy.
          </div>
          <div class="sound_control full-width">
            <div class="text-left">2:44</div>
            <div class="text-right">-1:03</div>
            <div class="track">
              <div class="active"></div>
            </div>
          </div>
        </div>
        <div class="search-misc-controls"></div>
      </header>
      <aside class="sidebar">
        <h3>SOURCES</h3>
        <ul id="source-menu" class="sidebar-menu">
          <li data-source="filesystem" class="active">Local</li>
          <li data-source="spotify">Spotify</li>
        </ul>
        <h3>DEVICES</h3>
        <ul id="deviceList" class="deviceList sidebar-menu"></ul>
      </aside>
      <div id="main">
        <div class="fs-wrapper">
          <input type="text" name="FS" class="fs-input full-width" id="FS" value="/">
          <div id="suggestion" class="suggestion"></div>
        </div>
        <hr>
        <ul id="fileList" class="fileList"></ul> 
        <hr>
      </div>
      <div class="cf"></div>
    </div>
    
  </body>
</html>